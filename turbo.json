{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "stream",
	"globalDependencies": ["**/.env.*local"],
	"globalPassThroughEnv": ["COREPACK_HOME", "PLAYWRIGHT_*"],
	"globalEnv": [
		"DEBUG",
		"NODE_ENV",
		"DATABASE_URL",
		"GITHUB_TOKEN",
		"ENABLE_EXPERIMENTAL_COREPACK",
		"GOOGLE_AI_API_KEY"
	],
	"tasks": {
		"//#format": {},
		"//#format:fix": {
			"cache": false
		},
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"clean": {
			"cache": false,
			"dependsOn": ["^clean"]
		},
		"@mjs/web#build": {
			"dependsOn": ["@mjs/i18n#build"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"],
			"env": [
				"EMAIL_API_KEY",
				"ALTCHA_HMAC_KEY",
				"EMAIL_FROM",
				"EMAILOCTOPUS_API_KEY",
				"EMAILOCTOPUS_LIST_ID",
				"MOBILE_OPTIMIZED"
			]
		},
		"@mjs/docs#build": {
			"dependsOn": ["@mjs/i18n#build"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**", "content/**"],
			"env": ["GITHUB_TOKEN", "ENABLE_REMOTE", "RENEW"]
		},
		"@mjs/token#build": {
			"dependsOn": ["@mjs/i18n#build"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"],
			"env": []
		},
		"@mjs/i18n#build": {
			"dependsOn": ["@mjs/utils#build"],
			"outputs": ["dist/**"]
		},
		"@mjs/utils#build": {
			"outputs": ["dist/**"]
		}
	}
}
